-- =========================
-- ANTI AFK
-- =========================
task.spawn(function()
    while task.wait(60) do
        if _G.Settings.AntiAFK then
            pcall(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
            end)
        end
    end
end)

-- =========================
-- CONTROLES DO HUB
-- =========================
Bubble.MouseButton1Click:Connect(function()
    Config.Aberto = not Config.Aberto
    
    if Config.Aberto then
        Main.Visible = true
        Tween(Main, 0.3, {Size = UDim2.fromOffset(IsMobile() and 380 or 500, IsMobile() and 520 or 600)})
    else
        Tween(Main, 0.3, {Size = UDim2.fromScale(0, 0)})
        task.wait(0.3)
        Main.Visible = false
    end
end)

CloseBtn.MouseButton1Click:Connect(function()
    Config.Aberto = false
    Tween(Main, 0.3, {Size = UDim2.fromScale(0, 0)})
    task.wait(0.3)
    Main.Visible = false
end)

-- =========================
-- SISTEMA DE BUSCA
-- =========================
Search:GetPropertyChangedSignal("Text"):Connect(function()
    local searchText = Search.Text:lower()
    
    for _, tab in pairs(Tabs) do
        for _, item in pairs(tab.Frame:GetChildren()) do
            if item:IsA("Frame") or item:IsA("TextButton") then
                local textLabel = item:FindFirstChildWhichIsA("TextLabel", true)
                if textLabel then
                    local itemText = textLabel.Text:lower()
                    item.Visible = searchText == "" or itemText:find(searchText) ~= nil
                end
            end
        end
    end
end)

