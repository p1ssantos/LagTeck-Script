    local url = "https://discord.gg/RnZ6XHHFj7"
    
    -- M√©todo 1: Copiar para clipboard
    pcall(function()
        setclipboard(url)
        print("‚úÖ Link do Discord copiado!")
    end)
    
    -- M√©todo 2: Mostrar no console
    print("üîó Link do Discord: " .. url)
    print("üìã Cole no seu navegador!")
    
    -- M√©todo 3: Abrir no navegador (para executores que suportam)
    pcall(function()
        request({
            Url = "http://127.0.0.1:6463/rpc?v=1",
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json",
                ["Origin"] = "https://discord.com"
            },
            Body = HttpService:JSONEncode({
                cmd = "INVITE_BROWSER",
                args = {code = "RnZ6XHHFj7"},
                nonce = HttpService:GenerateGUID(false)
            })
        })
    end)
end)

