Button(FruitTab, "üìä Loja Normal", function()
    task.spawn(function()
        local remote = GetRemote()
        if not remote then 
            warn("‚ùå Remote n√£o encontrado!")
            return 
        end
        
        print("=== LOJA NORMAL ===")
        print("Consultando frutas dispon√≠veis...")
        
        local success, result = pcall(function()
            return remote:InvokeServer("GetFruits")
        end)
        
        if success and result then
            if type(result) == "table" then
                for _, fruit in pairs(result) do
                    if type(fruit) == "table" and fruit.Name and fruit.Price then
                        print(string.format("üçé %s - üíµ $%s", fruit.Name, FormatNumber(fruit.Price)))
                    end
                end
            else
                print("‚ö†Ô∏è Formato de resposta inesperado")
            end
        else
            warn("‚ùå Erro ao consultar loja:", tostring(result))
        end
    end)
end)

Button(FruitTab, "üåÄ Loja Miragem", function()
    task.spawn(function()
        local remote = GetRemote()
        if not remote then 
            warn("‚ùå Remote n√£o encontrado!")
            return 
        end
        
        print("=== LOJA MIRAGEM ===")
        print("Consultando loja miragem...")
        
        local success, result = pcall(function()
            return remote:InvokeServer("GetFruits", true)
        end)
        
        if success then
            if type(result) == "table" then
                for _, fruit in pairs(result) do
                    if type(fruit) == "table" and fruit.Name and fruit.Price then
                        print(string.format("üåü %s - üíé %s fragmentos", fruit.Name, FormatNumber(fruit.Price)))
                    end
                end
            else
                print("‚úÖ Loja miragem verificada!")
            end
        else
            warn("‚ùå Erro ao consultar loja miragem:", tostring(result))
        end
    end)
end)

